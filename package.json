{
  "name": "stable-hash-x",
  "version": "0.2.0",
  "type": "module",
  "description": "Stable JS value hash.",
  "repository": "https://github.com/un-ts/stable-hash-x",
  "homepage": "https://github.com/un-ts/stable-hash-x#readme",
  "author": "Shu Ding",
  "maintainers": [
    "JounQin <admin@1stg.me> (https://www.1stG.me)"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.9.2",
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "./lib/index.cjs",
  "types": "./lib/index.d.cts",
  "module": "./lib/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "benchmark": "node benchmark > benchmark.txt",
    "build": "tsdown -d lib -f cjs,esm",
    "dev": "vitest",
    "docs": "vite",
    "docs:build": "vite build",
    "format": "prettier --write .",
    "lint": "run-p 'lint:*'",
    "lint:es": "eslint . --cache --max-warnings 10",
    "lint:tsc": "tsc --noEmit",
    "prepare": "simple-git-hooks && yarn-berry-deduplicate || exit 0",
    "release": "clean-pkg-json && changeset publish",
    "serve": "vite preview",
    "test": "vitest run",
    "typecov": "type-coverage",
    "version": "changeset version && yarn --no-immutable"
  },
  "devDependencies": {
    "@1stg/common-config": "^14.2.0",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.5",
    "@commitlint/cli": "^19.8.1",
    "@mdx-js/rollup": "^3.1.0",
    "@mitata/counters": "^0.0.8",
    "@shikijs/rehype": "^3.7.0",
    "@types/node": "^22.15.33",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/web": "^0.0.245",
    "@vercel/analytics": "^1.5.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/coverage-v8": "3.2.4",
    "ab64": "^0.1.6",
    "clean-pkg-json": "^1.3.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "flattie": "^1.1.1",
    "github-markdown-css": "^5.8.1",
    "hash-object": "^5.0.1",
    "json-stringify-deterministic": "^1.0.12",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "rehype-slug": "^6.0.0",
    "simple-git-hooks": "^2.13.0",
    "size-limit": "^11.2.0",
    "size-limit-preset-node-lib": "^0.4.0",
    "stable-hash": "^0.0.6",
    "tinybench": "^4.0.1",
    "tsdown": "^0.12.9",
    "type-coverage": "^2.29.7",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vitest": "^3.2.4",
    "yarn-berry-deduplicate": "^6.1.3"
  },
  "resolutions": {
    "prettier": "^3.6.0",
    "vite": "npm:rolldown-vite@^7.0.0"
  },
  "typeCoverage": {
    "atLeast": 100,
    "cache": true,
    "detail": true,
    "ignoreFiles": [
      "lib/**"
    ],
    "ignoreAsAssertion": true,
    "ignoreNonNullAssertion": true,
    "showRelativePath": true,
    "strict": true,
    "update": true
  }
}
